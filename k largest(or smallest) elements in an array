// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int bubble_sort(int a[],int length,int k){
    for(int i=0;i<length;i++){
        for(int j=0;j<length-1;j++){
            if(a[j]>a[j+1]){
                int temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;
            }
        }
    }
}
int main() {
    int a[]={2,14,5,21,8,6,7};
    int length=sizeof a/sizeof a[0];
    int k;
    cout<<"input K largest element:";
    cin>>k;
    bubble_sort(a,length,k);
    for(int i=length-1;i>(length-1-k);i--){
        cout<<"a["<<i<<"]="<<a[i]<<endl;
    }
    return 0;
}
