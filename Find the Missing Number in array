#include <iostream>
using namespace std;
int missing_num(int a[],int length){
    int n=length+1;
    int sum=n*(n+1)/2;
    for(int i=0;i<length;i++){
        sum-=a[i];
    }
    return sum;
}
int main() {
    int a[]={4,2,1,7,6,8,5};
    int length=sizeof a/ sizeof a[0];
    cout << missing_num(a,length);
    return 0;
}
