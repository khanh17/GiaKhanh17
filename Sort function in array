#include <iostream>
#include<bits/stdc++.h>
using namespace std;
void sort_fuction(int a[],int length){
    sort(a,a+length);
}
int main() {
    int a[]={2,4,9,8,7,21,12,3};
    int length=sizeof a/sizeof a[0];
    sort_fuction( a, length);
    for(int j=length-1;j>=0;j--){
         cout <<"[a"<<j<<"]=" <<a[j] <<endl;
    }
    return 0;
}
