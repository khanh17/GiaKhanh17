#include <iostream>
using namespace std;
int repeating_number(int a[],int length){
    for(int i=0;i<length;i++){
        for(int j=i+1;j<length;j++){
            if(a[i]==a[j]){
                return a[i];
            }
            
        }
    }
    return -1;
}
int main() {
    int a[]={3,4,6,7,10,12,2,6};
    int length=sizeof a/sizeof a[0];
    cout << repeating_number(a,length);
    return 0;
}
