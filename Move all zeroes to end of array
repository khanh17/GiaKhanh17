#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int move_all_zero(int a[],int length){
    sort(a,a+length);   
}
int main() {
    int a[] = { 5, 6, 0, 4, 6, 0, 9, 0, 8 };// Write C++ code here
    int length=sizeof a/sizeof a[0];
    int j=0;
    for(int i=0;i<length;i++){
        if(a[i]!=0){
            swap(a[i],a[j]);
            j++;
        }
    }
    for(int i=0;i < length;i++){
        cout <<"a[" <<i <<"]=" <<a[i]<<endl;
    }
    return 0;
}
